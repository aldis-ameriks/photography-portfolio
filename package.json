{
  "name": "photography-portfolio",
  "description": "Personal photography portfolio",
  "version": "1.0.0",
  "author": "Aldis Ameriks",
  "scripts": {
    "dev": "gatsby develop -o",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test start http://localhost:8000 cy:run",
    "docker:build": "docker build -t docker.pkg.github.com/aldis-ameriks/photography-portfolio/photography-portfolio .",
    "docker:push": "docker push docker.pkg.github.com/aldis-ameriks/photography-portfolio/photography-portfolio",
    "docker:build:push": "npm run docker:build && npm run docker:push",
    "docker:run": "docker run -p 8000:8000 --rm docker.pkg.github.com/aldis-ameriks/photography-portfolio/photography-portfolio"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@mdx-js/mdx": "^1.6.19",
    "@mdx-js/react": "^1.6.19",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.25.1",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-google-analytics": "^2.4.0",
    "gatsby-plugin-lodash": "^3.3.13",
    "gatsby-plugin-manifest": "^2.5.1",
    "gatsby-plugin-mdx": "^1.3.0",
    "gatsby-plugin-offline": "^3.3.1",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.13",
    "gatsby-plugin-sharp": "^2.7.0",
    "gatsby-plugin-sitemap": "^2.5.0",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-source-filesystem": "^2.4.0",
    "gatsby-transformer-sharp": "^2.5.21",
    "lodash": "^4.17.20",
    "polished": "^4.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-images": "^0.5.19",
    "react-spring": "^8.0.27",
    "styled-components": "^5.2.1",
    "styled-theming": "^2.2.0",
    "typeface-fira-sans": "^1.1.13",
    "typeface-montserrat": "^1.1.13"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^5.5.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "gatsby-cypress": "^0.4.12",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.11.5"
  },
  "homepage": "https://aldisameriks.photos",
  "bugs": {
    "url": "https://github.com/aldis-ameriks/photography-portfolio/issues"
  },
  "keywords": [
    "gatsby",
    "photography"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldis-ameriks/photography-portfolio.git"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md,mdx}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix --ext .js,.jsx --ignore-path .gitignore"
    ]
  }
}

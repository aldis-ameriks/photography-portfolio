{
  "name": "photography-portfolio",
  "description": "Personal photography portfolio",
  "version": "1.0.0",
  "author": "Aldis Ameriks",
  "homepage": "https://aldisameriks.photos",
  "bugs": {
    "url": "https://github.com/aldis-ameriks/photography-portfolio/issues"
  },
  "keywords": [
    "gatsby",
    "photography"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldis-ameriks/photography-portfolio.git"
  },
  "scripts": {
    "dev": "gatsby develop -o",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test start http://localhost:8000 cy:run",
    "docker:build": "docker build -t docker.pkg.github.com/aldis-ameriks/photography-portfolio/photography-portfolio .",
    "docker:push": "docker push docker.pkg.github.com/aldis-ameriks/photography-portfolio/photography-portfolio",
    "docker:build:push": "npm run docker:build && npm run docker:push",
    "docker:run": "docker run -p 8000:8000 --rm docker.pkg.github.com/aldis-ameriks/photography-portfolio/photography-portfolio"
  },
  "dependencies": {
    "@babel/core": "7.21.8",
    "@mdx-js/react": "2.3.0",
    "babel-plugin-styled-components": "2.1.1",
    "gatsby": "5.10.0",
    "gatsby-plugin-google-analytics": "5.10.0",
    "gatsby-plugin-image": "3.10.0",
    "gatsby-plugin-lodash": "6.10.0",
    "gatsby-plugin-manifest": "5.10.0",
    "gatsby-plugin-mdx": "5.10.0",
    "gatsby-plugin-offline": "6.10.0",
    "gatsby-plugin-react-helmet": "6.10.0",
    "gatsby-plugin-schema-export": "1.1.31",
    "gatsby-plugin-sharp": "5.10.0",
    "gatsby-plugin-sitemap": "6.10.0",
    "gatsby-plugin-styled-components": "6.10.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-filesystem": "5.10.0",
    "gatsby-transformer-sharp": "5.10.0",
    "jquery": "3.7.0",
    "justifiedGallery": "3.8.1",
    "lodash": "4.17.21",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-images": "0.5.19",
    "react-is": "18.2.0",
    "react-spring": "9.7.1",
    "styled-components": "5.3.10"
  },
  "devDependencies": {
    "@testing-library/cypress": "9.0.0",
    "@types/node": "20.2.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "cross-env": "7.0.3",
    "cypress": "12.12.0",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "gatsby-cypress": "3.10.0",
    "prettier": "2.8.8",
    "start-server-and-test": "2.0.0",
    "typescript": "5.0.4"
  }
}

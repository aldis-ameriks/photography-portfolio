{
  "name": "photography-portfolio",
  "description": "Personal photography portfolio",
  "version": "1.0.0",
  "author": "Aldis Ameriks",
  "scripts": {
    "dev": "gatsby develop -o",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test start http://localhost:8000 cy:run",
    "docker:build": "docker build -t docker.pkg.github.com/aldis-ameriks/photography-portfolio/photography-portfolio .",
    "docker:push": "docker push docker.pkg.github.com/aldis-ameriks/photography-portfolio/photography-portfolio",
    "docker:build:push": "npm run docker:build && npm run docker:push",
    "docker:run": "docker run -p 8000:8000 --rm docker.pkg.github.com/aldis-ameriks/photography-portfolio/photography-portfolio"
  },
  "dependencies": {
    "@babel/core": "^7.18.2",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "babel-plugin-styled-components": "^2.0.7",
    "gatsby": "^4.15.1",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-analytics": "^4.15.0",
    "gatsby-plugin-lodash": "^5.15.0",
    "gatsby-plugin-manifest": "^4.15.0",
    "gatsby-plugin-mdx": "^3.15.0",
    "gatsby-plugin-offline": "^5.15.0",
    "gatsby-plugin-react-helmet": "^5.15.0",
    "gatsby-plugin-remove-trailing-slashes": "^4.15.0",
    "gatsby-plugin-sharp": "^4.15.0",
    "gatsby-plugin-sitemap": "^5.15.0",
    "gatsby-plugin-styled-components": "^5.15.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-source-filesystem": "^4.15.0",
    "gatsby-transformer-sharp": "^4.15.0",
    "lodash": "^4.17.21",
    "polished": "^4.2.2",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-images": "0.5.19",
    "react-spring": "^9.4.5",
    "styled-components": "^5.3.5",
    "styled-theming": "^2.2.0",
    "typeface-fira-sans": "^1.1.13",
    "typeface-montserrat": "^1.1.13"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.7.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "gatsby-cypress": "^2.15.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0"
  },
  "homepage": "https://aldisameriks.photos",
  "bugs": {
    "url": "https://github.com/aldis-ameriks/photography-portfolio/issues"
  },
  "keywords": [
    "gatsby",
    "photography"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldis-ameriks/photography-portfolio.git"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md,mdx}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix --ext .js,.jsx --ignore-path .gitignore"
    ]
  }
}

{
  "name": "photography-portfolio",
  "description": "Personal photography portfolio",
  "version": "1.0.0",
  "author": "Aldis Ameriks",
  "scripts": {
    "dev": "gatsby develop -o",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test start http://localhost:8000 cy:run",
    "docker:build": "docker build -t docker.pkg.github.com/photography-portfolio/photography-portfolio .",
    "docker:push": "docker push docker.pkg.github.com/photography-portfolio/photography-portfolio",
    "docker:build:push": "npm run docker:build && npm run docker:push",
    "docker:run": "docker run -p 8000:8000 --rm docker.pkg.github.com/photography-portfolio/photography-portfolio"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.20.20",
    "gatsby-image": "^2.3.2",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-lodash": "^3.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-mdx": "^1.1.7",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-remove-trailing-slashes": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-sitemap": "^2.3.2",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.4.4",
    "lodash": "^4.17.15",
    "polished": "^3.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-images": "^0.5.19",
    "react-spring": "^8.0.27",
    "styled-components": "^5.1.0",
    "styled-theming": "^2.2.0",
    "typeface-fira-sans": "^0.0.75",
    "typeface-montserrat": "^0.0.75"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.3.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-cypress": "^0.3.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.2",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.11"
  },
  "homepage": "https://aldisameriks.photos",
  "bugs": {
    "url": "https://github.com/aldis-ameriks/photography-portfolio/issues"
  },
  "keywords": [
    "gatsby",
    "photography"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldis-ameriks/photography-portfolio.git"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md,mdx}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix --ext .js,.jsx --ignore-path .gitignore"
    ]
  }
}
